{
  "name": "traefik-ui",
  "version": "2.0.0",
  "description": "Traefik UI",
  "productName": "Traefik",
  "cordovaId": "us.containo.traefik",
  "private": true,
  "scripts": {
    "transfer": "node dev/scripts/transfer.js",
    "lint": "eslint --ext .js,.vue src",
    "test-unit": "mocha-webpack --mode=production './src/**/*.spec.js'",
    "dev": "export APP_ENV='development' && quasar dev",
    "build-quasar": "quasar build",
    "build-staging": "export NODE_ENV='production' && export APP_ENV='development' && npm run build-quasar && npm run build-quasar",
    "build": "export NODE_ENV='production' && export APP_ENV='production' && npm run build-quasar && npm run build-quasar && npm run transfer spa",
    "build:nc": "npm run build"
  },
  "dependencies": {
    "@quasar/extras": "*",
    "axios": "*",
    "bowser": "*",
    "chart.js": "*",
    "dot-prop": "*",
    "iframe-resizer": "*",
    "lodash.isequal": "*",
    "moment": "*",
    "quasar": "*",
    "query-string": "*",
    "vh-check": "*",
    "vue-chartjs": "*",
    "vuex-map-fields": "*",
    "@quasar/app": "*",
    "@vue/eslint-config-standard": "*",
    "@vue/test-utils": "*",
    "babel-eslint": "*",
    "chai": "*",
    "eslint": "*",
    "eslint-loader": "*",
    "eslint-plugin-prettier": "*",
    "eslint-plugin-mocha": "*",
    "eslint-plugin-vue": "*",
    "mocha": "*",
    "mocha-webpack": "*",
    "node-sass": "*",
    "prettier": "*",
    "sass-loader": "*"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "last 1 version, > 30%"
  ]
}
